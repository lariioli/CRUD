    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>aspnet-Tarefa-c2dd5cd3-92b9-48dc-bcfb-d41a8694ea82</UserSecretsId>
